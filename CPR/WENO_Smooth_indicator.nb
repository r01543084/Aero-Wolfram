Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["\[Beta]", 
     RowBox[{"j", " "}]], "=", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"l", "=", "1"}], "k"], 
     RowBox[{
      SubsuperscriptBox["\[Integral]", 
       SubscriptBox["x", 
        RowBox[{"i", "-", 
         RowBox[{"1", "/", "2"}]}]], 
       SubscriptBox["x", 
        RowBox[{"i", "+", 
         RowBox[{"1", "/", "2"}]}]]], 
      RowBox[{
       SuperscriptBox[
        RowBox[{
         SuperscriptBox["\[CapitalDelta]x", 
          RowBox[{
           RowBox[{"2", "l"}], "-", "1"}]], "(", 
         FractionBox[
          RowBox[{
           SuperscriptBox["\[PartialD]", "l"], 
           RowBox[{
            SubscriptBox["p", "j"], "[", "x", "]"}]}], 
          RowBox[{"\[PartialD]", 
           SuperscriptBox["x", "l"]}]], ")"}], "2"], 
       RowBox[{"\[DifferentialD]", "x"}]}]}]}]}], " ", ",", "         ", 
   RowBox[{"where", " ", "k", " ", "is", " ", "the", " ", "polynomial", " ", 
    RowBox[{"degree", "."}]}]}], TraditionalForm]]]], "Text",
