Cell[CellGroupData[{

Cell["Let us check that the answer is correct", "Subsection"],

Cell[TextData[{
 "First use ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " on one problem"
}], "Text"],

Cell[BoxData[
 FormBox[
  RowBox[{"DSolve", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"-", 
        RowBox[{
         RowBox[{"y", "''"}], "[", "x", "]"}]}], "\[Equal]", 
       RowBox[{"Sin", "[", 
        RowBox[{"\[Pi]", " ", 
         RowBox[{"x", "/", "L"}]}], "]"}]}], ",", 
      RowBox[{
