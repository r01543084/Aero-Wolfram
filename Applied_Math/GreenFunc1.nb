Notebook[{
Cell["Green functions with mathematica", "Title"],

Cell["vs 0.3, 25/1/07 \[Copyright] Niels Walet", "Subtitle",
 CellChangeTimes->{{3.410260205062748*^9, 3.410260222228375*^9}}],

Cell["\<\
Here we work two problems, and pose a final one, which you should try \
yourself. Double click the \"closed cells\" to show the full content. Use the \
help facility to figure out any unknown constructs.\
\>", "Text"],

Cell[CellGroupData[{

Cell["Problem 1", "Section"],

Cell[TextData[{
 "Find the Green function ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"G", "(", 
     RowBox[{"x", ",", 
      RowBox[{"x", "'"}]}], ")"}], " "}], TraditionalForm]]],
 " for the operator ",
 Cell[BoxData[
  FormBox["\[ScriptCapitalL]", TraditionalForm]]],
 " specified by ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[ScriptCapitalL]", " ", 
     RowBox[{"y", "(", "x", ")"}]}], "=", 
    RowBox[{
     RowBox[{"-", 
      FractionBox[
       SuperscriptBox["d", "2"], 
       RowBox[{"d", " ", 
        SuperscriptBox["x", "2"]}]]}], 
     RowBox[{"y", "(", "x", ")"}]}]}], TraditionalForm]]],
 ", in the range ",
 Cell[BoxData[
  FormBox[
   RowBox[{"0", "\[LessEqual]", "x", "\[LessEqual]", "L"}], 
   TraditionalForm]]],
 ", with boundary conditions ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"u", "(", "0", ")"}], "=", 
    RowBox[{
     RowBox[{"u", "(", "L", ")"}], "=", "0"}]}], TraditionalForm]]],
 ". Use as many different techniques as possible."
}], "Text",
 CellChangeTimes->{{3.410260230113233*^9, 3.410260258410163*^9}}],

Cell[CellGroupData[{

Cell["\<\
First we use the eigenvalue method; unfortunately Mathematica needs a little \
help..\
\>", "Subsection"],

Cell[BoxData[
 FormBox[
  RowBox[{"sol", "=", 
   RowBox[{
    RowBox[{"y", "[", "x", "]"}], "/.", 
    RowBox[{"First", "[", 
     RowBox[{"DSolve", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{
          RowBox[{"-", 
           RowBox[{
            RowBox[{"y", "''"}], "[", "x", "]"}]}], "\[Equal]", 
          RowBox[{"\[Lambda]", " ", 
           RowBox[{"y", "[", "x", "]"}]}]}], ",", 
         RowBox[{
          RowBox[{"y", "[", "0", "]"}], "\[Equal]", "0"}]}], "}"}], ",", 
       RowBox[{"y", "[", "x", "]"}], ",", "x"}], "]"}], "]"}]}]}], 
  TraditionalForm]], "Input"],

Cell["\<\
(The \"First\" gets rid of some curly brackets generated by mathematica, the \
\"/.\" operator substitutes the result).
Clearly we find\
\>", "Text"],

Cell[BoxData[
 FormBox[
  RowBox[{"Solve", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"L", " ", 
      SqrtBox["\[Lambda]"]}], "\[Equal]", 
     RowBox[{"n", " ", "\[Pi]"}]}], ",", "\[Lambda]"}], "]"}], 
  TraditionalForm]], "Input"],

Cell[BoxData[
 FormBox[
  RowBox[{"sol", " ", "=", " ", 
   RowBox[{"a", " ", 
    RowBox[{"Sin", "[", 
     RowBox[{"n", " ", "\[Pi]", " ", 
      RowBox[{"x", " ", "/", "L"}]}], "]"}]}]}], TraditionalForm]], "Input"],

Cell["Normalise the solutions", "Text"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubsuperscriptBox["\[Integral]", "0", "L"], 
    RowBox[{
     SuperscriptBox["sol", "2"], 
     RowBox[{"\[DifferentialD]", "x"}]}]}], "\[Equal]", "1"}], 
  TraditionalForm]], "Input"],

Cell[TextData[{
 "Use the fact that ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]]],
 " is an integer (which mathematica doesn't know)"
}], "Text"],

Cell[BoxData[
 FormBox[
  RowBox[{"Solve", "[", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       RowBox[{
        SubsuperscriptBox["\[Integral]", "0", "L"], 
        RowBox[{
         SuperscriptBox["sol", "2"], 
         RowBox[{"\[DifferentialD]", "x"}]}]}], "\[Equal]", "1"}], "/.", 
      RowBox[{
       RowBox[{"Sin", "[", 
        RowBox[{"2", "n", " ", "\[Pi]"}], "]"}], "\[Rule]", "0"}]}], ")"}], 
    ",", "a"}], "]"}], TraditionalForm]], "Input"],

Cell["Pick one of these two, and conclude that", "Text"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"G", "(", 
    RowBox[{"x", ",", 
     RowBox[{"x", "'"}]}], ")"}], "=", 
   RowBox[{
    UnderoverscriptBox["\[Sum]", 
     RowBox[{"n", "=", "1"}], "\[Infinity]"], 
    RowBox[{
     FractionBox[
      SuperscriptBox["L", "2"], 
      RowBox[{
       SuperscriptBox["n", "2"], " ", 
       SuperscriptBox["\[Pi]", "2"]}]], 
     FractionBox["2", "L"], 
     RowBox[{"sin", "(", 
      FractionBox[
       RowBox[{"n", " ", "\[Pi]", " ", "x"}], "L"], ")"}], 
     RowBox[{"sin", "(", 
      FractionBox[
       RowBox[{"n", " ", "\[Pi]", " ", 
        RowBox[{"x", "'"}]}], "L"], ")"}]}]}]}], 
  TraditionalForm]], "DisplayFormula"],

Cell[TextData[{
 "We use xp rather than ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "'"}], TraditionalForm]]],
 ", since ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " assigns a special meaning to primes:"
}], "Text"],

Cell[BoxData[
 FormBox[
  RowBox[{"Plot3D", "[", 
   RowBox[{
    RowBox[{
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{"n", "=", "1"}], "10"], 
      RowBox[{
       FractionBox[
        SuperscriptBox["L", "2"], 
        RowBox[{
         SuperscriptBox[
          RowBox[{"n", " "}], "2"], 
         SuperscriptBox["\[Pi]", "2"]}]], 
       FractionBox["2", "L"], 
       RowBox[{"Sin", "[", 
        FractionBox[
         RowBox[{"n", " ", "\[Pi]", " ", "x"}], "L"], "]"}], 
       RowBox[{"Sin", "[", 
        FractionBox[
         RowBox[{"n", " ", "\[Pi]", " ", "xp"}], "L"], "]"}]}]}], "/.", 
     RowBox[{"L", "\[Rule]", "1"}]}], ",", 
    RowBox[{"{", 
     RowBox[{"x", ",", "0", ",", "1"}], "}"}], ",", 
    RowBox[{"{", 
     RowBox[{"xp", ",", "0", ",", "1"}], "}"}], ",", 
    RowBox[{"AxesLabel", "\[Rule]", 
     RowBox[{"{", 
      RowBox[{"x", ",", 
       RowBox[{"x", "'"}], ",", "G"}], "}"}]}]}], "]"}], 
  TraditionalForm]], "Input"],

Cell["\<\
If you so wish this can be simplified; mathematica will even do the sum for \
you...\
\>", "Text"],

Cell[BoxData[
 FormBox[
  RowBox[{"Simplify", "[", 
   RowBox[{
    UnderoverscriptBox["\[Sum]", 
     RowBox[{"n", "=", "1"}], "\[Infinity]"], 
    RowBox[{
     FractionBox[
      SuperscriptBox["L", "2"], 
      RowBox[{
       SuperscriptBox[
        RowBox[{"n", " "}], "2"], 
       SuperscriptBox["\[Pi]", "2"]}]], 
     FractionBox["2", "L"], 
     RowBox[{"Sin", "[", 
      FractionBox[
       RowBox[{"n", " ", "\[Pi]", " ", "x"}], "L"], "]"}], 
     RowBox[{"Sin", "[", 
      FractionBox[
       RowBox[{"n", " ", "\[Pi]", " ", "z"}], "L"], "]"}]}]}], "]"}], 
  TraditionalForm]], "Input"],

Cell["\<\
This is a subtle sum of two complex logarithms; if we plot it we see it makes \
sense, but we have to remove small numerical errors that lead to imaginary \
parts.\
\>", "Text"],

Cell[BoxData[
 FormBox[
  RowBox[{"Plot3D", "[", 
   RowBox[{
    RowBox[{"Re", "[", 
     RowBox[{
      FractionBox[
       RowBox[{"L", " ", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{
           SubscriptBox[
            InterpretationBox["Li",
             PolyLog,
             Editable->False,
             Selectable->False], "2"], "(", 
           SuperscriptBox["\[ExponentialE]", 
            RowBox[{"-", 
             FractionBox[
              RowBox[{"\[ImaginaryI]", " ", "\[Pi]", " ", 
               RowBox[{"(", 
                RowBox[{"x", "-", "z"}], ")"}]}], "L"]}]], ")"}], "+", 
          RowBox[{
           SubscriptBox[
            InterpretationBox["Li",
             PolyLog,
             Editable->False,
             Selectable->False], "2"], "(", 
           SuperscriptBox["\[ExponentialE]", 
            FractionBox[
             RowBox[{"\[ImaginaryI]", " ", "\[Pi]", " ", 
              RowBox[{"(", 
               RowBox[{"x", "-", "z"}], ")"}]}], "L"]], ")"}], "-", 
          RowBox[{
           SubscriptBox[
            InterpretationBox["Li",
             PolyLog,
             Editable->False,
             Selectable->False], "2"], "(", 
           SuperscriptBox["\[ExponentialE]", 
            RowBox[{"-", 
             FractionBox[
              RowBox[{"\[ImaginaryI]", " ", "\[Pi]", " ", 
               RowBox[{"(", 
                RowBox[{"x", "+", "z"}], ")"}]}], "L"]}]], ")"}], "-", 
          RowBox[{
           SubscriptBox[
            InterpretationBox["Li",
             PolyLog,
             Editable->False,
             Selectable->False], "2"], "(", 
           SuperscriptBox["\[ExponentialE]", 
            FractionBox[
             RowBox[{"\[ImaginaryI]", " ", "\[Pi]", " ", 
              RowBox[{"(", 
               RowBox[{"x", "+", "z"}], ")"}]}], "L"]], ")"}]}], ")"}]}], 
       RowBox[{"2", " ", 
        SuperscriptBox["\[Pi]", "2"]}]], "/.", 
      RowBox[{"L", "\[Rule]", "1"}]}], "]"}], ",", 
    RowBox[{"{", 
     RowBox[{"x", ",", "0", ",", "1"}], "}"}], ",", 
    RowBox[{"{", 
     RowBox[{"z", ",", "0", ",", "1"}], "}"}], ",", 
    RowBox[{"AxesLabel", "\[Rule]", 
     RowBox[{"{", 
      RowBox[{"x", ",", 
       RowBox[{"x", "'"}], ",", "G"}], "}"}]}]}], "]"}], 
  TraditionalForm]], "Input"]
}, Open  ]],

Cell[CellGroupData[{

