Cell["Determinants", "Subsection"],

Cell[TextData[{
 "The determinant of a square matrix is given by an expansion (\"the \
Laplacian development by minors\"). First we assign each entry a sign (",
 Cell[BoxData[
  FormBox["+", TraditionalForm]]],
 " or ",
 Cell[BoxData[
  FormBox["-", TraditionalForm]]],
 ") starting with a ",
 Cell[BoxData[
  FormBox["+", TraditionalForm]]],
 "  in the top left corner of the matrix, making sure that each direct \
neighbour, in row or column, has the opposite sign, i.e., of the following \
form"
}], "Text",
 CellChangeTimes->{{3.410258732827859*^9, 3.410258733928854*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"(", GridBox[{
      {"+", "-", "+", "-"},
      {"-", "+", "-", "+"},
      {"+", "-", "+", "-"},
      {"-", "+", "-", "+"}
     }], ")"}], ".", " "}], TraditionalForm]], "DisplayFormula",
 CellChangeTimes->{{3.410258732827859*^9, 3.4102587374810658`*^9}}],

Cell["\<\
We then pick one row or a column, take each element of the row or column in \
turn, multiply it by its sign and by the determinant with all elements in the \
row and column of the current element removed, until we end up with a 1\
\[Times]1 matrix. Let's look at that, expanding the following matrix by the \
second column\
\>", "Text",
 CellChangeTimes->{{3.410258732827859*^9, 3.410258741520561*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"|", GridBox[{
      {"1", "2", "3", "4"},
      {"5", "4", "1", "2"},
      {"6", "8", "0", "3"},
      {"4", "7", "2", "1"}
     }], "|"}], "=", 
   RowBox[{
    RowBox[{"-", "2"}], "|", GridBox[{
      {"5", "1", "2"},
      {"6", "0", "3"},
      {"4", "2", "1"}
     }], "|", 
    RowBox[{"+", "4"}], "|", GridBox[{
      {"1", "3", "4"},
      {"6", "0", "3"},
      {"4", "2", "1"}
     }], "|", 
    RowBox[{"-", "8"}], "|", GridBox[{
      {"1", "3", "4"},
      {"5", "1", "2"},
      {"4", "2", "1"}
     }], "|", 
    RowBox[{"+", "7"}], "|", GridBox[{
      {"1", "3", "4"},
      {"5", "1", "2"},
      {"6", "0", "3"}
     }], "|", 
    RowBox[{".", " "}]}]}], TraditionalForm]], "DisplayFormula",
 CellChangeTimes->{{3.410258732827859*^9, 3.410258748719819*^9}}],

Cell["\<\
Now expand all of the determinants w.r.t. to the middle column,\
\>", "Text",
 CellChangeTimes->{{3.410258732827859*^9, 3.410258751799474*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"|", GridBox[{
      {"1", "2", "3", "4"},
      {"5", "4", "1", "2"},
      {"6", "8", "0", "3"},
      {"4", "7", "2", "1"}
     }], "|"}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"-", "2"}], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"-", "1"}], "|", GridBox[{
          {"6", "3"},
          {"4", "1"}
         }], "|", 
        RowBox[{"-", "2"}], "|", GridBox[{
          {"5", "2"},
          {"6", "3"}
         }], "|"}], ")"}]}], "+", 
     RowBox[{"4", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"-", "3"}], "|", GridBox[{
          {"6", "3"},
          {"4", "1"}
         }], "|", 
        RowBox[{"-", "2"}], "|", GridBox[{
          {"1", "4"},
          {"6", "3"}
         }], "|"}], ")"}]}], "-", 
     RowBox[{"8", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"-", "3"}], "|", GridBox[{
          {"5", "2"},
          {"4", "1"}
         }], "|", 
        RowBox[{"+", "1"}], "|", GridBox[{
          {"1", "4"},
          {"4", "1"}
         }], "|", 
        RowBox[{"-", "2"}], "|", GridBox[{
          {"1", "4"},
          {"5", "2"}
         }], "|"}], ")"}]}], "+", 
     RowBox[{"7", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"-", "3"}], "|", GridBox[{
          {"5", "2"},
          {"6", "3"}
         }], "|", 
        RowBox[{"+", "1"}], "|", GridBox[{
          {"1", "4"},
          {"6", "3"}
         }], "|"}], ")"}]}]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"-", "2"}], 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"-", "1"}], 
          RowBox[{"(", 
           RowBox[{"-", "6"}], ")"}]}], "-", 
         RowBox[{"2", 
          RowBox[{"(", "3", ")"}]}]}], ")"}]}], "+", 
      RowBox[{"4", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"-", "3"}], 
          RowBox[{"(", 
           RowBox[{"-", "6"}], ")"}]}], "-", 
         RowBox[{"2", 
          RowBox[{"(", 
           RowBox[{"-", "21"}], ")"}]}]}], ")"}]}], "-", 
      RowBox[{"8", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"-", "3"}], 
          RowBox[{"(", 
           RowBox[{"-", "3"}], ")"}]}], "+", 
         RowBox[{"1", 
          RowBox[{"(", 
           RowBox[{"-", "15"}], ")"}]}], "-", 
         RowBox[{"2", 
          RowBox[{"(", 
           RowBox[{"-", "18"}], ")"}]}]}], ")"}]}], "+", 
      RowBox[{"7", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"-", "3"}], 
          RowBox[{"(", "3", ")"}]}], "+", 
         RowBox[{"1", 
          RowBox[{"(", 
           RowBox[{"-", "21"}], ")"}]}]}], ")"}]}]}], "=", 
     RowBox[{"-", "210"}]}]}]}], TraditionalForm]], "DisplayFormula",
 CellChangeTimes->{{3.410258732827859*^9, 3.4102587564627438`*^9}}],

Cell["Check using Mathematica", "Text",
