   RowBox[{
    RowBox[{"Sin", "[", 
     RowBox[{
      SubscriptBox["k", "x"], "x"}], "]"}], 
    RowBox[{"Sin", "[", 
     RowBox[{
      SubscriptBox["k", "y"], "y"}], "]"}], 
