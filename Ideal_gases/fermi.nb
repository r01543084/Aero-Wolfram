Cell["\<\
The chemical potential is related to the temperature and density by\
\>", "Text"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"Clear", "[", "eq1", "]"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"eq1", "[", 
   RowBox[{"z_", ",", "\[Tau]_"}], "]"}], "=", 
  RowBox[{"PowerExpand", "[", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{
       SubscriptBox["n", "Q"], "\[Equal]", 
       RowBox[{"fermi", "[", 
        RowBox[{
         FractionBox["3", "2"], ",", "z"}], "]"}]}], "/.", "lambdaToT"}], "/.", 
     RowBox[{"{", 
      RowBox[{"T", "\[Rule]", 
       FractionBox[
        RowBox[{"\[Tau]", " ", 
         SubscriptBox["\[CurlyEpsilon]", "F"]}], 
        SubscriptBox["k", "B"]]}], "}"}]}], "/.", "eFrule"}], 
   "]"}]}]}], "Input",
 CellLabel->"In[84]:="],

Cell[BoxData[
 RowBox[{
  FractionBox["4", 
   RowBox[{"3", " ", 
    SqrtBox["\[Pi]"], " ", 
    SuperscriptBox["\[Tau]", 
     RowBox[{"3", "/", "2"}]]}]], "==", 
  RowBox[{"-", 
   RowBox[{"PolyLog", "[", 
    RowBox[{
     FractionBox["3", "2"], ",", 
     RowBox[{"-", "z"}]}], "]"}]}]}]], "Output",
 CellLabel->"Out[85]="]
}, Closed]],

Cell[TextData[{
 "where we express energy and chemical potential in units of ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[CurlyEpsilon]", "F"], TraditionalForm]]],
 " and temperature as ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Tau]", "=", 
    RowBox[{"T", "/", 
     SubscriptBox["T", "F"]}]}], TraditionalForm]]],
 ".  Although this equation usually must be solved numerically, it will be \
useful to develop symbolic relationship valid for either low or high \
temperature limits.  In both cases we must be careful to select the proper \
root and not to use the expansion too far outside its range of validity."
}], "Text"],

