Cell[CellGroupData[{

Cell["Small z expansion", "Subsection"],

Cell[TextData[{
 "Inspection of the Taylor series around ",
 Cell[BoxData[
  FormBox[
   RowBox[{"z", "\[Rule]", "0"}], TraditionalForm]]]
}], "Text"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Series", "[", 
  RowBox[{
   RowBox[{"fermi", "[", 
    RowBox[{"\[Nu]", ",", "z"}], "]"}], ",", 
   RowBox[{"{", 
    RowBox[{"z", ",", "0", ",", "3"}], "}"}]}], "]"}]], "Input",
 CellLabel->"In[63]:="],

Cell[BoxData[
 InterpretationBox[
  RowBox[{"z", "-", 
   RowBox[{
    SuperscriptBox["2", 
     RowBox[{"-", "\[Nu]"}]], " ", 
    SuperscriptBox["z", "2"]}], "+", 
   RowBox[{
    SuperscriptBox["3", 
     RowBox[{"-", "\[Nu]"}]], " ", 
    SuperscriptBox["z", "3"]}], "+", 
   InterpretationBox[
    SuperscriptBox[
     RowBox[{"O", "[", "z", "]"}], "4"],
    SeriesData[z, 0, {}, 1, 4, 1]]}],
  SeriesData[z, 0, {1, -2^(-\[Nu]), 3^(-\[Nu])}, 1, 4, 1]]], "Output",
 CellLabel->"Out[63]="]
}, Closed]],

Cell["suggests that the general power series takes the form", "Text"],

Cell[BoxData[
 RowBox[{
  RowBox[{"fermiZexpansion", "[", 
   RowBox[{"\[Nu]_", ",", "z_", ",", 
    RowBox[{
     SubscriptBox["k", "max"], ":", "_"}]}], "]"}], ":=", 
  RowBox[{"-", 
   RowBox[{
    UnderoverscriptBox["\[Sum]", 
     RowBox[{"k", "=", "1"}], 
     SubscriptBox["k", "max"]], 
    FractionBox[
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"-", "z"}], ")"}], "k"], 
     SuperscriptBox["k", "\[Nu]"]]}]}]}]], "Input",
 CellLabel->"In[64]:="],

Cell["\<\
Below we verify that the infinite series does reproduce the desired function.\
\>", "Text"],

