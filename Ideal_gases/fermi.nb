    SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
    RowBox[{
     FractionBox[
      SuperscriptBox["x", 
       RowBox[{"\[Nu]", "-", "1"}]], 
      RowBox[{
       RowBox[{
        SuperscriptBox["z", 
         RowBox[{"-", "1"}]], 
        RowBox[{"Exp", "[", "x", "]"}]}], "+", "1"}]], 
     RowBox[{"\[DifferentialD]", "x"}]}]}]}]}]], "DisplayFormula",
 Evaluatable->False,
 TextAlignment->Center,
 TextJustification->0,
 FontFamily->"Times New Roman"],

