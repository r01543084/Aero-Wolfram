Cell["Degeneracy pressure", "Subsubsection"],

Cell[TextData[{
 "The mechanical equation of state is obtained directly from the grand \
potential, ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalG]", " ", "=", " ", 
    RowBox[{"p", " ", "V"}]}], TraditionalForm]]],
 ".  It is useful to express the mechanical equation of state in the form ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{"p", " ", "V"}], 
     RowBox[{"N", " ", 
      SubscriptBox["k", "B"], "T"}]], "=", 
    RowBox[{"B", "[", "\[Tau]", "]"}]}], TraditionalForm]]],
 ", where ",
 StyleBox["B",
  FontSlant->"Italic"],
 " is sometimes called the ",
 StyleBox["compressibility factor",
  FontSlant->"Italic"],
 "."
}], "Text"],

