Cell[TextData[{
 StyleBox["Rules for Changing variables", "Section"],
 "\n",
 Cell[BoxData[{
  RowBox[{
   RowBox[{"TtoLambda", "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"T", "\[Rule]", 
       FractionBox[
        RowBox[{"2", " ", "\[Pi]", " ", 
         SuperscriptBox["\[HBar]", "2"]}], 
        RowBox[{"m", " ", 
         SubscriptBox["k", "B"], " ", 
         SuperscriptBox["\[Lambda]", "2"]}]]}], ",", " ", 
      RowBox[{"\[Beta]", "\[Rule]", 
       FractionBox["1", 
        RowBox[{
         SubscriptBox["k", "B"], "T"}]]}]}], "}"}]}], ";"}], "\n", 
  RowBox[{
   RowBox[{"lambdaToT", "=", 
    RowBox[{"{", 
     RowBox[{"\[Lambda]", "\[Rule]", 
      SqrtBox[
       FractionBox[
        RowBox[{"2", " ", "\[Pi]", " ", 
         SuperscriptBox["\[HBar]", "2"]}], 
        RowBox[{"m", " ", 
         SubscriptBox["k", "B"], " ", "T"}]]]}], "}"}]}], ";"}], "\n", 
  RowBox[{
   RowBox[{"lambdaToZ", "=", 
    RowBox[{"{", 
     RowBox[{"\[Lambda]", "\[Rule]", 
      SuperscriptBox[
       RowBox[{"(", 
        FractionBox[
         RowBox[{"g", " ", "V", " ", 
          RowBox[{"fermi", "[", 
           RowBox[{
            FractionBox["3", "2"], ",", "z"}], "]"}]}], "\[ScriptCapitalN]"], 
        ")"}], 
       RowBox[{"1", "/", "3"}]]}], "}"}]}], ";"}]}]],
 "\[LineSeparator]",
 Cell[BoxData[{
  RowBox[{
   RowBox[{"zToTau", "=", 
    RowBox[{"z", "\[Rule]", 
     RowBox[{"fugacity", "[", "\[Tau]", "]"}]}]}], ";"}], "\n", 
  RowBox[{
   RowBox[{"muToZ", "=", 
    RowBox[{"\[Mu]", " ", "\[Rule]", 
     RowBox[{
      SubscriptBox["k", "B"], "T", " ", 
      RowBox[{"Log", "[", "z", "]"}]}]}]}], ";"}], "\n", 
  RowBox[{
   RowBox[{"zToT", " ", "=", " ", 
    RowBox[{"z", "\[Rule]", 
     RowBox[{"Exp", "[", 
      FractionBox["\[Mu]", 
       RowBox[{
        SubscriptBox["k", "B"], "T"}]], "]"}]}]}], ";"}]}]]
}], "Text",
 CellChangeTimes->{{3.577499075699549*^9, 3.5774990774906516`*^9}, {
